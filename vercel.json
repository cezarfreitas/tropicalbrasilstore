{
  "functions": {
    "server/production.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/production.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ]
}
